{
  "name": "react-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "test": "react-scripts test",
    "test:cv": "yarn run test -- --coverage",
    "y:lint": "eslint . --ext .ts --ext .tsx",
    "y:pret": "prettier --write .",
    "local:ci": "react-scripts test --watchAll=false",
